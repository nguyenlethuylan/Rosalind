   "source": [
    "# Rabbits_and_Recurrence_Relations\n",
    "def total_rabbits(n, k):\n",
    "    rabbits = [0] * n  \n",
    "    rabbits[0:2] = 1, 1 \n",
    "    for i in range(2, n): \n",
    "        rabbits[i] = rabbits[i - 1] + rabbits[i - 2] * k  \n",
    "    return rabbits[-1]  \n",
    "\n",
    "total_rabbits(32,2)"
   ]
  }
